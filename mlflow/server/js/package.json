  "scripts": {
    "//": "IMPORTANT: For compatibility with Windows in MLflow OSS, do NOT inline environment variables in scripts other than `start-web`, `start:mfe` and `test:ci`. Environment variables can instead be set in `mlflow/server/js/scripts/global-setup.js`.",
    "start": "craco start",
    "start:iframe": "PUBLIC_URL=/ SERVE_WEBAPP=true HIDE_HEADER=true HIDE_EXPERIMENT_LIST=true SHOW_GDPR_PURGING_MESSAGES=true USE_ABSOLUTE_AJAX_URLS=true SHOULD_REDIRECT_IFRAME=true craco start",
    "start:mfe": "MLFLOW_MFE_DEV=true npm run start:iframe",
    "start-web": "SERVE_WEBAPP=true HIDE_HEADER=true HIDE_EXPERIMENT_LIST=true SHOW_GDPR_PURGING_MESSAGES=true USE_ABSOLUTE_AJAX_URLS=true SHOULD_REDIRECT_IFRAME=true PORT=3001 craco start",
    "build": "craco --max_old_space_size=8192 build",
    "build:mfe": "MLFLOW_MFE_DEV=true craco --max_old_space_size=8192 build",
    "test": "craco test --env=jsdom --colors",
    "test:ci": "CI=true craco test --env=jsdom --colors --forceExit --ci",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache --env=jsdom",
    "eject": "react-scripts eject",
    "gocx-codegen": "../../../js/scripts/bin/gocx-codegen ../..",
    "gocx-codegen:check": "../../../js/scripts/bin/gocx-codegen --check",
    "extract-i18n:base": "node scripts/extract-i18n.js",
    "extract-i18n": "npm run extract-i18n:base",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "//": "homepage is hard to configure without resorting to env variables and doesn't play nicely with other webpack settings. This field should be removed.",
  "homepage": "static-files",
  "browserslist": [
    "defaults"
  ],
  "babel": {
    "env": {
      "test": {
        "plugins": [
          [
            "babel-plugin-formatjs",
            {
              "idInterpolationPattern": "[sha512:contenthash:base64:6]",
              "removeDefaultMessage": false
            }
          ]
        ]
      }
    }
  }
